{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ProjectManagementproject": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/project-managementproject",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/examples.css",
              "src/assets/css/style.css",
              "src/assets/vendors/simplebar/css/simplebar.css"
            ],
            "scripts": [
              "src/assets/js/main.js",
              "src/assets/js/calendar.js",
              "src/assets/js/charts.js",
              "src/assets/js/color-modes.js",
              "src/assets/js/colors.js",
              "src/assets/js/config.js",
              "src/assets/js/datatables.js",
              "src/assets/js/google-maps.js",
              "src/assets/js/i18next.js",
              "src/assets/js/loading-buttons.js",
              "src/assets/js/multi-select.js",
              "src/assets/js/popovers.js",
              "src/assets/js/toasts.js",
              "src/assets/js/tooltips.js",
              "src/assets/js/widgets.js",
              "src/assets/vendors/jquery/js/jquery.min.js",
              "src/assets/vendors/simplebar/js/simplebar.min.js",
              "src/assets/vendors/i18next-http-backend/js/i18nextHttpBackend.js",
              "src/assets/vendors/i18next-browser-languagedetector/js/i18nextBrowserLanguageDetector.js",
              "src/assets/vendors/i18next/js/i18next.min.js",
              "src/assets/vendors/fullcalendar/js/index.global.min.js",
              "src/assets/vendors/datatables.net-bs5/js/dataTables.bootstrap5.min.js",
              "src/assets/vendors/datatables.net/js/dataTables.min.js"


            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ProjectManagementproject:build:production"
            },
            "development": {
              "browserTarget": "ProjectManagementproject:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ProjectManagementproject:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
